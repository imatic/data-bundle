{
    "name": "imatic/data-bundle",
    "type": "symfony-bundle",
    "description": "Imatic Data Bundle",
    "keywords": [],
    "homepage": "http://imatic.cz",
    "license": "proprietary",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "http://composer.imatic.cz"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/framework-bundle": "~2.7|~3.0",
        "doctrine/doctrine-bundle": "~1.2",
        "genemu/form-bundle": "~2.2.2"
    },
    "require-dev": {
        "imatic/testing-bundle": "~2.7|~3.0",
        "imatic/form-bundle": "~2.7|~3.0",
        "doctrine/orm": "~2.3",
        "mikey179/vfsStream": "~1.4",
        "phpunit/phpunit": "~5.2"
    },
    "suggest": {
        "genemu/form-bundle": "*",
        "doctrine/dbal": "*",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\DataBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "secure-http": false
    }
}
