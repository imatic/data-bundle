{
    "name": "imatic/data-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to work with data",
    "keywords": ["data", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "https://www.imatic.cz"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "^3.4|^4.0",
        "doctrine/doctrine-bundle": "^1.2"
    },
    "require-dev": {
        "imatic/testing": "dev-parklane",
        "imatic/form-bundle": "dev-parklane",
        "doctrine/orm": "^2.3",
        "mikey179/vfsStream": "^1.4",
        "symfony/phpunit-bridge": "^4.1",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpmd/phpmd": "^2.6",
        "phpdocumentor/reflection-docblock": "4.1.1 as 3.3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "suggest": {
        "doctrine/dbal": "*",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\DataBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
