{
    "name": "imatic/data-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to work with data",
    "keywords": ["data", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "https://www.imatic.cz"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/framework-bundle": "^5.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/orm": "^2.3",
        "friendsofphp/php-cs-fixer": "^2.7",
        "imatic/form-bundle": "dev-update_deps",
        "imatic/testing": "dev-update_deps",
        "mikey179/vfsstream": "^1.4",
        "phpdocumentor/reflection-docblock": "4.1.1 as 3.3.0",
        "symfony/phpunit-bridge": "^4.1"
    },
    "suggest": {
        "doctrine/dbal": "*",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\DataBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
