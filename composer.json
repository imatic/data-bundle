{
    "name": "imatic/data-bundle",
    "type": "symfony-bundle",
    "description": "Bundle to work with data",
    "keywords": ["data", "symfony"],
    "homepage": "https://www.imatic.cz",
    "license": "MIT",
    "authors": [
        {
            "name": "Stepan Koci",
            "email": "stepan.koci@imatic.cz"
        },
        {
            "name": "Imatic Software s.r.o.",
            "homepage": "http://imatic.cz/"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.imatic.cz"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/framework-bundle": "~3.3",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "require-dev": {
        "imatic/testing-bundle": "~3.0",
        "imatic/form-bundle": "~3.0",
        "doctrine/orm": "~2.3",
        "mikey179/vfsStream": "~1.4",
        "symfony/phpunit-bridge": "~3.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "phpmd/phpmd": "^2.6"
    },
    "suggest": {
        "doctrine/dbal": "*",
        "doctrine/orm": "*"
    },
    "autoload": {
        "psr-4": {
            "Imatic\\Bundle\\DataBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
